{
  "name": "specialkey-emulator-cli",
  "appId": "de.sidneys.specialkey-emulator-cli",
  "version": "0.9.0",
  "description": "Emulate special keys from the command line.",
  "license": "MIT",
  "homepage": "https://github.com/sidneys/specialkey-emulator-cli",
  "author": {
    "name": "sidneys",
    "email": "sidneys.github.io@outlook.com",
    "url": "https://sidneys.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sidneys/specialkey-emulator-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sidneys/specialkey-emulator-cli/issues"
  },
